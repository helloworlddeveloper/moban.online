<!-- BEGIN: main --><link type="text/css" href="{NV_BASE_SITEURL}{NV_ASSETS_DIR}/js/jquery-ui/jquery-ui.min.css" rel="stylesheet" /><link rel="stylesheet" href="{NV_BASE_SITEURL}{NV_ASSETS_DIR}/js/select2/select2.min.css"><!-- BEGIN: view --><form class="form-inline" action="{NV_BASE_ADMINURL}index.php" method="get">    <input type="hidden" name="{NV_LANG_VARIABLE}"  value="{NV_LANG_DATA}" />    <input type="hidden" name="{NV_NAME_VARIABLE}"  value="{MODULE_NAME}" />    <input type="hidden" name="{NV_OP_VARIABLE}"  value="{OP}" />    <table class="table table-striped table-bordered table-hover">        <tbody>        <tr>            <td>                <select style="width: 100%;" class="form-control" name="status">                    <option value="-1">{LANG.status_search}</option>                    <!-- BEGIN: status_select -->                    <option value="{OPTION.key}" {OPTION.selected}>{OPTION.title}</option>                    <!-- END: status_select -->                </select>            </td>            <td>                <select style="width: 100%;" onchange="nv_get_district(this.value, 0)" class="form-control" name="catid">                    <option value="0">{LANG.cat_search}</option>                    <!-- BEGIN: cat_search -->                    <option value="{OPTION.id}" {OPTION.selected}>{OPTION.title}</option>                    <!-- END: cat_search -->                </select>            </td>            <td>                <input class="form-control" placeholder="{LANG.search_title}" style="width: 300px;" type="text" value="{Q}" name="q" maxlength="255" />&nbsp;                <input class="btn btn-primary" type="submit" value="{LANG.search}" />                &nbsp;<a href="{addcontact}" class="btn btn-primary">{LANG.addcontact}</a>            </td>        </tr>        </tbody>    </table></form><br /><form class="form-inline" action="{NV_BASE_ADMINURL}index.php?{NV_LANG_VARIABLE}={NV_LANG_DATA}&amp;{NV_NAME_VARIABLE}={MODULE_NAME}&amp;{NV_OP_VARIABLE}={OP}" method="post">    <div class="table-responsive">        <table class="table table-striped table-bordered table-hover">            <thead>            <tr>                <th>{LANG.title_event}</th>                <th>{LANG.moderator}</th>                <th>{LANG.addressevent}</th>                <th>{LANG.group_name}</th>                <th>{LANG.group_cat}</th>                <th>{LANG.timeevent}</th>                <th>{LANG.timefix_title}</th>                <th>{LANG.status}</th>                <th>&nbsp;</th>            </tr>            </thead>            <tfoot>            <tr>                <td colspan="7">{NV_GENERATE_PAGE}</td>            </tr>            </tfoot>            <tbody>            <!-- BEGIN: loop -->            <tr>                <td> {VIEW.title} </td>                <td> {VIEW.moderator} </td>                <td> {VIEW.addressevent} </td>                <td> {VIEW.group_title} </td>                <td> {VIEW.cat_title} </td>                <td> {VIEW.timeevent_begin} - {VIEW.timeevent_end}  {VIEW.timeevent} </td>                <td> {VIEW.timefix} </td>                <td> {VIEW.status} </td>                <td class="text-center">                    <i class="fa fa-edit fa-lg">&nbsp;</i> <a href="{VIEW.link_edit}">{LANG.edit}</a>                    - <em class="fa fa-trash-o fa-lg">&nbsp;</em> <a href="{VIEW.link_delete}" onclick="return confirm(nv_is_del_confirm[0]);">{LANG.delete}</a>                </td>            </tr>            <!-- END: loop -->            </tbody>        </table>    </div></form><!-- END: view --><!-- BEGIN: add_row --><!-- BEGIN: error --><div class="alert alert-warning">{ERROR}</div><!-- END: error --><form action="{FORM_ACTION}" method="post" class="form-inline confirm-reload">    <input name="submit" type="hidden" value="1" />    <input name="id" type="hidden" value="{DATA.id}" />    <div class="row">        <div class="table-responsive">            <table class="table table-striped table-bordered table-hover">                <colgroup>                    <col class="w200" />                    <col />                </colgroup>                <tbody>                <tr>                    <td class="text-right"> {LANG.title_event} <sup class="required">(*)</sup></td>                    <td><input class="w300 form-control pull-left" type="text" value="{DATA.title}" name="title" id="idtitle" maxlength="250" />&nbsp</td>                </tr>                <tr>                    <td class="text-right"> {LANG.moderator} <sup class="required">(*)</sup></td>                    <td><input class="w300 form-control pull-left" type="text" value="{DATA.moderator}"  maxlength="50" name="moderator" />&nbsp</td>                </tr>                <tr>                    <td class="text-right"> {LANG.participants}</td>                    <td><input class="w300 form-control pull-left" type="text" value="{DATA.participants}"  maxlength="50" name="participants" />&nbsp</td>                </tr>                <tr>                    <td class="text-right">                        {LANG.timeevent}  <sup class="required">(*)</sup>                    </td>                    <td>                        {LANG.from}                        <select name="timeevent_hour" class="check_time_exits form-control">                            <option value="0">---{LANG.hour}---</option>                            <!-- BEGIN: timeevent_hour -->                            <option value="{HOUR.index}"{HOUR.selected}>{HOUR.index}</option>                            <!-- END: timeevent_hour -->                        </select>&nbsp;                        <select name="timeevent_minute" class="check_time_exits form-control">                            <option value="0">---{LANG.minute}---</option>                            <!-- BEGIN: timeevent_minute -->                            <option value="{MINUTE.index}"{MINUTE.selected}>{MINUTE.index}</option>                            <!-- END: timeevent_minute -->                        </select>                        {LANG.to}                        <select name="timeclose_hour" class="check_time_exits form-control">                            <option value="0">---{LANG.hour}---</option>                            <!-- BEGIN: timeclose_hour -->                            <option value="{HOUR.index}"{HOUR.selected}>{HOUR.index}</option>                            <!-- END: timeclose_hour -->                        </select>&nbsp;                        <select name="timeclose_minute" class="check_time_exits form-control">                            <option value="0">---{LANG.minute}---</option>                            <!-- BEGIN: timeclose_minute -->                            <option value="{MINUTE.index}"{MINUTE.selected}>{MINUTE.index}</option>                            <!-- END: timeclose_minute -->                        </select>                        <input class="w100 check_time_exits form-control" type="text" placeholder="{LANG.day}" value="{DATA.timeevent_str}" id="timeevent" name="timeevent" />                    </td>                </tr>                <tr>                    <td class="text-right">                        {LANG.addressevent}  <sup class="required">(*)</sup>                    </td>                    <td><input class="w300 form-control" type="text" value="{DATA.addressevent}" name="addressevent" /></td>                </tr>                <tr>                    <td class="text-right">                        {LANG.select_catid}  <sup class="required">(*)</sup>                    </td>                    <td>                        <select name="catid" class="check_time_exits form-control">                            <option value="0">---{LANG.select_catid}---</option>                            <!-- BEGIN: catid -->                            <option value="{CAT.id}"{CAT.sl}>{CAT.title}</option>                            <!-- END: catid -->                        </select>                    </td>                </tr>                <tr>                    <td class="text-right">                        {LANG.select_groupid}  <sup class="required">(*)</sup>                    </td>                    <td>                        <select name="groupid" class="check_time_exits form-control">                            <option value="0">---{LANG.select_groupid}---</option>                            <!-- BEGIN: groupid -->                            <option value="{GROUP.id}"{GROUP.sl}>{GROUP.title}</option>                            <!-- END: groupid -->                        </select>                    </td>                </tr>                <tr>                    <td class="text-right">                        {LANG.provinceid}  <sup class="required">(*)</sup>                    </td>                    <td>                        <select name="provinceid" class="form-control">                            <option value="0">---{LANG.province_select}---</option>                            <!-- BEGIN: province -->                            <option value="{PROVINCE.id}"{PROVINCE.sl}>{PROVINCE.title}</option>                            <!-- END: province -->                        </select>                    </td>                </tr>                <tr>                    <td class="text-right">                    </td>                    <td>                        <label id="timefix"><input type="checkbox" name="timefix" value="1"{timefix}>{LANG.timefix}</label>                        <div id="result_reposive" style="color: #f00;padding: 10px"></div>                    </td>                </tr>                <tr>                    <td colspan="2">                        <div> <strong>{LANG.description}</strong></div>                        {description}                    </td>                </tr>                </tbody>            </table>        </div>    </div>    <div class="row text-center">        <input type="submit" value="{LANG.save}" class="btn btn-primary"/>    </div></form><script type="text/javascript" src="{NV_BASE_SITEURL}{NV_ASSETS_DIR}/js/jquery-ui/jquery-ui.min.js"></script><script type="text/javascript" src="{NV_BASE_SITEURL}{NV_ASSETS_DIR}/js/language/jquery.ui.datepicker-{NV_LANG_INTERFACE}.js"></script><script>    $("#timeevent").datepicker({        showOn : "focus",        dateFormat : "dd/mm/yy",        changeMonth : true,        changeYear : true,        showOtherMonths : true,        buttonImage : nv_base_siteurl + "assets/images/calendar.gif",        buttonImageOnly : true    });    check_time_exits();    $('.check_time_exits').blur(function() {        check_time_exits();    });    function check_time_exits() {        var timeevent_hour = $('select[name=timeevent_hour]').val();        var timeevent_minute = $('select[name=timeevent_minute]').val();        var timeclose_hour = $('select[name=timeclose_hour]').val();        var timeclose_minute = $('select[name=timeclose_minute]').val();        var timeevent = $('input[name=timeevent]').val();        var calendarid = $('input[name=id]').val();        var catid = $('select[name=catid]').val();        var groupid = $('select[name=groupid]').val();        $.post(script_name + '?' + nv_name_variable + '=' + nv_module_name + '&' + nv_fc_variable + '=main&nocache=' + new Date().getTime(), 'checktime=1&calendarid=' + calendarid + ' &timeevent_hour=' + timeevent_hour            + '&timeevent_minute=' + timeevent_minute + '&timeclose_hour=' + timeclose_hour + '&timeclose_minute=' + timeclose_minute + '&timeevent='+ timeevent + '&catid=' + catid + '&groupid=' + groupid, function(res) {            if (res != "") {                $('#result_reposive').html( res );                $('input[name=timefix]').val(0);                $('#timefix').hide();            } else {                $('#timefix').show();                $('#result_reposive').html('');            }        });    }</script><!-- END: add_row --><!-- END: main -->