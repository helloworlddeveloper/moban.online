<!-- BEGIN: main --><!-- BEGIN: data --><form class="form-inline" action="{NV_BASE_ADMINURL}index.php" method="get">    <input type="hidden" name="{NV_LANG_VARIABLE}"  value="{NV_LANG_DATA}" />    <input type="hidden" name="{NV_NAME_VARIABLE}"  value="{MODULE_NAME}" />    <input type="hidden" name="{NV_OP_VARIABLE}"  value="{OP}" />    <div class="panel">        <div class="panel-default">            <select  class="form-control" name="provinceid">                <option value="0">{LANG.province_search}</option>                <!-- BEGIN: province_select -->                <option value="{OPTION.id}" {OPTION.selected}>{OPTION.title}</option>                <!-- END: province_select -->            </select>            <input class="btn btn-primary" type="submit" value="{LANG.search_submit}" />            <a class="btn btn-primary" href="{add_events}">{LANG.add_events}</a>        </div>    </div></form><div class="table-responsive">    <table class="table table-striped table-bordered table-hover">        <thead>        <tr class="text-center">            <th>{LANG.title_event}</th>            <th>{LANG.timeevent}</th>            <th>{LANG.addressevent}</th>            <th>{LANG.provinceid}</th>            <th>{LANG.status}</th>            <th>{LANG.feature}</th>        </tr>        </thead>        <tbody>        <!-- BEGIN: row -->        <tr>            <td>{ROW.title}</td>            <td>{ROW.timeevent}</td>            <td>{ROW.addressevent}</td>            <td>{ROW.provinceid}</td>            <td class="text-center">                <select id="change_status_{ROW.id}" onchange="nv_chang_status('{ROW.id}', '{op}');" class="form-control">                    <!-- BEGIN: status -->                    <option value="{STATUS.key}"{STATUS.selected}>{STATUS.val}</option>                    <!-- END: status -->                </select></td>            <td class="text-center"><i class="fa fa-envelope-o">&nbsp;</i><a href="{ROW.url_sms}">SMS</a> &nbsp; <em class="fa fa-edit fa-lg">&nbsp;</em><a href="{ROW.url_edit}">{GLANG.edit}</a> &nbsp; <em class="fa fa-trash-o fa-lg">&nbsp;</em><a href="javascript:void(0);" onclick="nv_module_del({ROW.id}, '{op}', '{ROW.checkss}');">{GLANG.delete}</a></td>        </tr>        <!-- END: row -->        </tbody>    </table></div><!-- END: data --><!-- BEGIN: add --><link type="text/css" href="{NV_BASE_SITEURL}{NV_ASSETS_DIR}/js/jquery-ui/jquery-ui.min.css" rel="stylesheet" /><link rel="stylesheet" href="{NV_BASE_SITEURL}{NV_ASSETS_DIR}/js/select2/select2.min.css"><!-- BEGIN: error --><div class="alert alert-danger">    {ERROR}</div><!-- END: error --><form action="{FORM_ACTION}" method="post" class="form-inline confirm-reload">    <input name="submit" type="hidden" value="1" />    <div class="row">        <div class="table-responsive">            <table class="table table-striped table-bordered table-hover">                <colgroup>                    <col class="w200" />                    <col />                </colgroup>                <tbody>                <tr>                    <td class="text-right"> {LANG.title_event} <sup class="required">(*)</sup></td>                    <td><input class="w300 form-control pull-left" type="text" value="{DATA.title}" name="title" id="idtitle" maxlength="250" />&nbsp</td>                </tr>                <tr>                    <td class="text-right">                        {LANG.timeevent}  <sup class="required">(*)</sup>                    </td>                    <td>                        <select name="timeevent_hour" class="form-control">                            <option value="0">---{LANG.hour_select}---</option>                            <!-- BEGIN: timeevent_hour -->                            <option value="{HOUR.index}"{HOUR.selected}>{HOUR.index}</option>                            <!-- END: timeevent_hour -->                        </select>&nbsp;                        <select name="timeevent_minute" class="form-control">                            <option value="0">---{LANG.minute_select}---</option>                            <!-- BEGIN: timeevent_minute -->                            <option value="{MINUTE.index}"{MINUTE.selected}>{MINUTE.index}</option>                            <!-- END: timeevent_minute -->                        </select>                        <input class="w300 form-control" type="text" value="{DATA.timeeventf}" id="timeevent" name="timeevent" />                    </td>                </tr>                <tr>                    <td class="text-right">                        {LANG.timeclose}                    </td>                    <td>                        <select name="timeclose_hour" class="form-control">                            <option value="0">---{LANG.hour_select}---</option>                            <!-- BEGIN: timeclose_hour -->                            <option value="{HOUR.index}"{HOUR.selected}>{HOUR.index}</option>                            <!-- END: timeclose_hour -->                        </select>&nbsp;                        <select name="timeclose_minute" class="form-control">                            <option value="0">---{LANG.minute_select}---</option>                            <!-- BEGIN: timeclose_minute -->                            <option value="{MINUTE.index}"{MINUTE.selected}>{MINUTE.index}</option>                            <!-- END: timeclose_minute -->                        </select>                        <input class="w300 form-control" type="text" value="{DATA.timeclosef}" id="timeclose" name="timeclose" />                    </td>                </tr>                <tr>                    <td class="text-right">                        {LANG.contactname}  <sup class="required">(*)</sup>                    </td>                    <td><input class="w300 form-control" type="text" value="{DATA.contactname}" name="contactname" /></td>                </tr>                <tr>                    <td class="text-right">                        {LANG.contactmobile}  <sup class="required">(*)</sup>                    </td>                    <td><input class="w300 form-control" type="text" value="{DATA.contactmobile}" name="contactmobile" /></td>                </tr>                <tr>                    <td class="text-right">                        {LANG.addressevent}  <sup class="required">(*)</sup>                    </td>                    <td><input class="w300 form-control" type="text" value="{DATA.addressevent}" name="addressevent" /></td>                </tr>                <tr>                    <td class="text-right">                        {LANG.provinceid}  <sup class="required">(*)</sup>                    </td>                    <td>                        <select name="provinceid" class="form-control">                            <option value="0">---{LANG.province_select}---</option>                            <!-- BEGIN: province -->                            <option value="{PROVINCE.id}"{PROVINCE.sl}>{PROVINCE.title}</option>                            <!-- END: province -->                        </select>                    </td>                </tr>                <tr>                    <td colspan="2">                        <div> <strong>{LANG.description_event}</strong></div>                        {description}                    </td>                </tr>                </tbody>            </table>        </div>    </div>    <div class="row text-center">        <input type="submit" value="{LANG.save}" class="btn btn-primary"/>    </div></form><script type="text/javascript" src="{NV_BASE_SITEURL}{NV_ASSETS_DIR}/js/jquery-ui/jquery-ui.min.js"></script><script type="text/javascript" src="{NV_BASE_SITEURL}{NV_ASSETS_DIR}/js/language/jquery.ui.datepicker-{NV_LANG_INTERFACE}.js"></script><script>    $("#timeevent,#timeclose").datepicker({        showOn : "focus",        dateFormat : "dd/mm/yy",        changeMonth : true,        changeYear : true,        showOtherMonths : true,        buttonImage : nv_base_siteurl + "assets/images/calendar.gif",        buttonImageOnly : true    });</script><!-- END: add --><!-- END: main -->